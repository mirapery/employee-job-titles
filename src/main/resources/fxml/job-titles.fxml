<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="JobController">

   <Label fx:id="title" text="%title">
      <font>
         <Font size="24.0"/>
      </font>
   </Label>
   <ComboBox fx:id="languageSelector" prefWidth="150.0"/>
   <ListView fx:id="job_titles" prefHeight="200.0" prefWidth="200.0"/>
   <Button fx:id="fetch_button" mnemonicParsing="false" text="%button" onAction="#searchJobTitles"/>
   <Label fx:id="error_label"/>
</VBox>
